<Window x:Class="WPF_Tool.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="clr-namespace:System.Windows.Interop;assembly=PresentationCore"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"

    Title="MainWindow" Height="768" Width="1024" Background="Black">
    <Grid >
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="2*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="200"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="250"/>
        </Grid.ColumnDefinitions>
        <DockPanel Grid.ColumnSpan="3">
            <Menu DockPanel.Dock="Top">
                <MenuItem Header="_File" Height="30">
                    <MenuItem Header="_New" />
                    <MenuItem Header="_Open"/>
                    <MenuItem Header="_Save"/>
                    <Separator/>
                    <MenuItem Header="_Exit"/>
                </MenuItem>
                <MenuItem Header="Data" Height="30" >
                    <MenuItem Header="_Import Scene" Click="BTN_ImportScene"/>
                    <MenuItem Header="_Import Hierarchy"  Click="BTN_ImportHierarchy"/>
                    <MenuItem Header="_Import MapData" Click="BTN_ImportMapData"/>
                    <MenuItem Header="_Import NavMesh" Click="BTN_ImportNavMesh"/>
                    <MenuItem Header="_Import Terrain" Click="BTN_ImportTerrain"/>
                    <MenuItem Header="_Import Light" Click="BTN_ImportLight"/>
                    <MenuItem Header="_Import Effect" />
                    <Separator/>
                    <MenuItem Header="_Export Scene" Click="BTN_ExportScene"/>
                    <MenuItem Header="_Export Hierarchy" Click="BTN_ExportHierarchy"/>
                    <MenuItem Header="_Export MapData"  Click="BTN_ExportMapData"/>
                    <MenuItem Header="_Export NavMesh"  Click="BTN_ExportNavMesh"/>
                    <MenuItem Header="_Export Terrain"  Click="BTN_ExportTerrain"/>
                    <MenuItem Header="_Export Light"  Click="BTN_ExportLight"/>
                    <MenuItem Header="_Export Effect" />
                </MenuItem>
                <MenuItem Header="GameObject" Height="30" >
                    <MenuItem Header="_CreateMesh" Click="BTN_CreateMesh"/>
                    <MenuItem Header="_CreatePawn" Click="BTN_CreatePawn"/>
                    <MenuItem Header="_CreateMapObject"  Click="BTN_CreateMapObject"/>
                    <MenuItem Header="_CreateNavMesh" Click="BTN_CreateNavMesh"/>
                    <MenuItem Header="_CreateTerrain" Click="BTN_CreateTerrain"/>
                    <MenuItem Header="_CreateLight" Click="BTN_CreateLight" />
                    <MenuItem Header="_CreateMeshEffect" Click="BTN_CreateMeshEffect" />
                    <MenuItem Header="_CreateTextureEffect" Click="BTN_CreateTextureEffect" />
                    <MenuItem Header="_CreateUIPanel"  Click="BTN_CreateUIPanel" />
                    <MenuItem Header="_CreateEffect" />
                    <MenuItem Header="_CreateCamera"/>
                
                </MenuItem>
                <MenuItem Header="Component" Height="30" >
                </MenuItem>
                <MenuItem Header="Help" Height="30" >
                </MenuItem>
            </Menu>
        </DockPanel>

        <StackPanel Orientation="Horizontal" Grid.ColumnSpan="3">
        </StackPanel>
        <TabControl Grid.Row ="1">
            <TabItem Header="Hierarchy">

                <StackPanel>
                    <!-- 여기서 하이어라키 -->
                    <ListBox x:Name="HierarchyList"
                         HorizontalAlignment="Left"
                         VerticalAlignment="Top" Height="450" Width="198">
                        <ListBox.ContextMenu>
                            <ContextMenu>
                                <MenuItem Header="_CreateMesh" Foreground="Black" Click="BTN_CreateMesh"/>
                                <MenuItem Header="_CreatePawn" Foreground="Black"  Click="BTN_CreatePawn"/>
                                <MenuItem Header="_CreateMapObject" Foreground="Black" Click="BTN_CreateMapObject"/>
                                <MenuItem Header="_CreateNavMesh" Foreground="Black" Click="BTN_CreateNavMesh"/>
                                <MenuItem Header="_CreateTerrain" Foreground="Black" Click="BTN_CreateTerrain"/>
                                <MenuItem Header="_CreateLight" Foreground ="Black" Click="BTN_CreateLight" />
                                <MenuItem Header="_CreateMeshEffect" Foreground="Black" Click="BTN_CreateMeshEffect" />
                                <MenuItem Header="_CreateTextureEffect" Foreground="Black" Click="BTN_CreateTextureEffect" />
                                <MenuItem Header="_CreateUIPanel"  Foreground="Black"  Click="BTN_CreateUIPanel" />
                                <MenuItem Header="_CreateEffect" Foreground="Black" />
                                <MenuItem Header="_CreateCamera" Foreground="Black" />
                            </ContextMenu>
                        </ListBox.ContextMenu>
                    </ListBox>
                </StackPanel>
            </TabItem>
        </TabControl>
        <TabControl Grid.Row="1" Grid.Column="1" SelectionChanged="SceneTab_SelectionChanged">
            <TabItem Header="Scene">
                <StackPanel>
                    <StackPanel Orientation="Horizontal" >
                        <ToggleButton x:Name="ViewToolButton" Width="25" Height="20">
                            <Image Source="Images/Eye.png"></Image>
                        </ToggleButton>
                        <ToggleButton x:Name="HandToolButton" Width="25" Height="20">
                            <Image Source="Images/Hand.png"></Image>
                        </ToggleButton>
                        <ToggleButton x:Name="PickToolButton" Width="25" Height="20">
                            <Image Source="Images/Pick.png"></Image>
                        </ToggleButton>
                        <ToggleButton x:Name="NavMeshToolButton" Width="25" Height="20">
                            <Image Source="Images/NavMesh.png"></Image>
                        </ToggleButton>
                        <TextBlock Name="RenderState" Width="80" VerticalAlignment="Center" Foreground="White" TextAlignment="Center">
                            RenderState
                        </TextBlock>
                        <ComboBox Name="RenderStateBox" Width="100" VerticalAlignment="Top"  SelectedIndex="0">
                            <ComboBoxItem Content="Shaded" Selected="Solid_Selected"/>
                            <ComboBoxItem Content="WireFrame" Selected="WireFrame_Selected"/>
                        </ComboBox>
                        <TextBlock Width="10"></TextBlock>
                        <TextBlock VerticalAlignment="Center" Foreground="White" Width="90">ColliderViewer</TextBlock>
                        <ComboBox Name="ColliderViewMode" Width="100" VerticalAlignment="Top" SelectedIndex="0">
                            <ComboBoxItem Content="All" Selected="WireFrame_Selected"/>
                            <ComboBoxItem Content="ForEditor" Selected="Solid_Selected"/>
                            <ComboBoxItem Content="ForGame" Selected="WireFrame_Selected"/>
                            <ComboBoxItem Content="Collisionable" Selected="Solid_Selected"/>
                            <ComboBoxItem Content="Dynamic" Selected="WireFrame_Selected"/>
                            <ComboBoxItem Content="Static" Selected="Solid_Selected"/>
                            <ComboBoxItem Content="Multipurpose" Selected="WireFrame_Selected"/>
                            <ComboBoxItem Content="Selectable" Selected="WireFrame_Selected"/>
                            <ComboBoxItem Content="Nothing" Selected="WireFrame_Selected"/>
                        </ComboBox>
                        <TextBlock Width="20"/>
                        <!--빈 공간-->
                        <TextBlock Width="40" VerticalAlignment="Center" TextAlignment="Center" Foreground="White"> Gizmo</TextBlock>
                        <CheckBox VerticalAlignment="Center"/>
                    </StackPanel>
                    <Image x:Name="imgelt"
                           MouseMove="Dx_MouseMove" MouseDown="Dx_MouseDown" MouseUp="Dx_MouseUp">
                        <Image.Source>
                            <i:D3DImage x:Name="d3dimg"/>
                        </Image.Source>
                    </Image>
                </StackPanel>

            </TabItem>
            <TabItem Header="Game">
                <StackPanel>
                    <Image x:Name="imgelt_Game"
                           MouseMove="Dx_MouseMove" MouseDown="Dx_MouseDown" MouseUp="Dx_MouseUp">
                        <Image.Source>
                            <i:D3DImage x:Name="d3dimg_Game"/>
                        </Image.Source>
                    </Image>
                </StackPanel>
            </TabItem>
        </TabControl>
        <TabControl x:Name="InspectorTab" Grid.Row="1" Grid.Column="2" Grid.RowSpan="2">
            <TabItem Header="Inspector">
                <!-- Inspector -->
                <StackPanel>
                    <ScrollViewer VerticalScrollBarVisibility="Visible" Height="685">
                        <StackPanel  x:Name="InspectorStackPanel">
                            <GroupBox x:Name="ObjectData" IsEnabled="True" Visibility="Visible">
                                <GroupBox.Header>
                                    <Label Width="100">Object Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="10"></TextBlock>
                                        <CheckBox x:Name="ActiveCheckBox" VerticalAlignment="Center" HorizontalAlignment="Center"
                                  Checked="Active_Checked"
                                  Unchecked="Active_Unchecked"  
                                  IsChecked="True"/>
                                        <TextBlock Width="10"></TextBlock>
                                        <TextBlock Width="50" Foreground="White" VerticalAlignment="Center" HorizontalAlignment="Center">Name</TextBlock>
                                        <TextBox Width="120" x:Name="GameObjectName" TextChanged="GameObjectName_TextChanged" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="10"></TextBlock>
                                        <CheckBox x:Name="FixCheckBox" VerticalAlignment="Center" HorizontalAlignment="Center"
                                  Checked="Fix_Checked"
                                  Unchecked="Fix_Unchecked"  
                                  IsChecked="False"/>
                                        <TextBlock Width="10"></TextBlock>
                                        <TextBlock Foreground="White" Width="50" VerticalAlignment="Center" HorizontalAlignment="Center">Tag</TextBlock>
                                        <ComboBox  x:Name="ObjectTag" Width="120" VerticalAlignment="Top" SelectedIndex="0" SelectionChanged="Tag_Selected">
                                            <ComboBoxItem Content="Default" />
                                            <ComboBoxItem Content="UI"/>
                                            <ComboBoxItem Content="Player" />
                                            <ComboBoxItem Content="Enemy"/>
                                        </ComboBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="40"></TextBlock>
                                        <TextBlock Foreground="White" Width="50" VerticalAlignment="Center" HorizontalAlignment="Center">Layer</TextBlock>
                                        <ComboBox  x:Name="ObjectLayer" Width="120" VerticalAlignment="Top" SelectedIndex="0" SelectionChanged="Layer_Selected">
                                            <ComboBoxItem Content="Default" />
                                            <ComboBoxItem Content="UI"/>
                                            <ComboBoxItem Content="Player" />
                                            <ComboBoxItem Content="Enemy"/>
                                        </ComboBox>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="40"></TextBlock>
                                        <TextBlock Foreground="White" Width="50" VerticalAlignment="Center" HorizontalAlignment="Center">Static</TextBlock>
                                        <ComboBox x:Name="ObjectStatic" Width="120" VerticalAlignment="Top" SelectedIndex="0" SelectionChanged="Static_Selected" >
                                            <ComboBoxItem Content="Nothing" />
                                            <ComboBoxItem Content="Navigation"/>
                                        </ComboBox>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                            
                            <GroupBox x:Name="TransformData" IsEnabled="True" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label Width="100">Transform Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock >Position</TextBlock>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionX_MouseDown">X</TextBlock>
                                        <TextBox x:Name="PositionX" Width="50" TextChanged="PositionX_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionY_MouseDown">Y</TextBlock>
                                        <TextBox x:Name="PositionY" Width="50" TextChanged="PositionY_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">Z</TextBlock>
                                        <TextBox x:Name="PositionZ" Width="50" TextChanged="PositionZ_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock>Rotation</TextBlock>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center"  MouseDown="RotationX_MouseDown">X</TextBlock>
                                        <TextBox x:Name="RotationX" Width="50" TextChanged="RotationX_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="RotationY_MouseDown">Y</TextBlock>
                                        <TextBox x:Name="RotationY" Width="50" TextChanged="RotationY_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="RotationZ_MouseDown">Z</TextBlock>
                                        <TextBox x:Name="RotationZ" Width="50" TextChanged="RotationZ_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock>Scale</TextBlock>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="ScaleX_MouseDown">X</TextBlock>
                                        <TextBox x:Name="ScaleX" Width="50" TextChanged="ScaleX_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="ScaleY_MouseDown">Y</TextBlock>
                                        <TextBox x:Name="ScaleY" Width="50" TextChanged="ScaleY_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="ScaleZ_MouseDown">Z</TextBlock>
                                        <TextBox x:Name="ScaleZ" Width="50" TextChanged="ScaleZ_TextChanged">0</TextBox>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>

                            <GroupBox x:Name="MapData" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label>MapData</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="10"></TextBlock>
                                        <TextBlock Foreground="White" Width="50" VerticalAlignment="Center" HorizontalAlignment="Center">Group</TextBlock>
                                        <ComboBox  x:Name="MapGroupComboBox" Width="140" VerticalAlignment="Top" SelectedIndex="0" SelectionChanged="SLT_MapGroup">
                                            <ComboBoxItem Content="Group0" />
                                            <ComboBoxItem Content="Group1"/>
                                            <ComboBoxItem Content="Group2" />
                                            <ComboBoxItem Content="Group3"/>
                                            <ComboBoxItem Content="Group4"/>
                                            <ComboBoxItem Content="Group5"/>
                                            <ComboBoxItem Content="Group6"/>
                                            <ComboBoxItem Content="Group7"/>
                                            <ComboBoxItem Content="Group8"/>
                                            <ComboBoxItem Content="Group9"/>
                                        </ComboBox>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="MeshData" IsEnabled="True" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label>Mesh Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel  Orientation="Horizontal">
                                        <TextBlock Width="55"><Run Text="MeshFile"/></TextBlock>
                                        <TextBox x:Name="MeshFilePath" Width="100" />
                                        <TextBlock Width="5"/>
                                        <Button Width="60" Click="BTN_MeshFile" Content="Load"/>
                                    </StackPanel>
                                    <TextBlock/>
                                    <StackPanel  Orientation="Horizontal">
                                        <TextBlock Width="55">Diffuse</TextBlock>
                                        <TextBox x:Name="DiffuseFilePath" Width="100" />
                                        <TextBlock Width ="5"/>
                                        <Button Width="60" Click="BTN_Diffuse">Load</Button>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="TerrainData" IsEnabled="True" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label>Terrain Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Height="5"/>
                                    <TextBlock Width="90" HorizontalAlignment="Left" Margin="10, 0, 0, 0">VertexInfo</TextBlock>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="30" HorizontalAlignment="Center" TextAlignment="Center">X</TextBlock>
                                        <TextBox x:Name="TerrainX" Width="30" TextAlignment="Center"
                                                 TextChanged="TerrainX_TextChanged">10</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center">Y</TextBlock>
                                        <TextBox x:Name="TerrainZ" Width="30" TextAlignment="Center"
                                                 TextChanged="TerrainZ_TextChanged">10</TextBox>
                                        <TextBlock Width="60" HorizontalAlignment="Center" TextAlignment="Center">Interval</TextBlock>
                                        <TextBox  x:Name="TerrainInterval" Width="30" TextAlignment="Center"
                                                 TextChanged="TerrainInterval_TextChanged">1</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <TextBlock Width="90" HorizontalAlignment="Left" Margin="10, 0, 0, 0">TextureInfo</TextBlock>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="30" HorizontalAlignment="Center" TextAlignment="Center">X</TextBlock>
                                        <TextBox x:Name="TerrainTextureX" Width="30" TextAlignment="Center"
                                                 TextChanged="TerrainTextureX_TextChanged">10</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center">Y</TextBlock>
                                        <TextBox x:Name="TerrainTextureZ" Width="30" TextAlignment="Center"
                                                 TextChanged="TerrainTextureZ_TextChanged">10</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <TextBlock Height="5"/>
                                    <StackPanel  Orientation="Horizontal">
                                        <TextBlock Width="55"><Run Text="Diffuse"/></TextBlock>
                                        <TextBox x:Name="TerrainDiffuseFilePath" Width="100" />
                                        <TextBlock Width="5"/>
                                        <Button Width="60" Click="BTN_TerrainDiffuseLoad" Content="Load"/>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel  Orientation="Horizontal">
                                        <TextBlock Width="55">Normal</TextBlock>
                                        <TextBox x:Name="TerrainNormalFilePath" Width="100" />
                                        <TextBlock Width ="5"/>
                                        <Button Width="60" Click="BTN_TerrainNormalLoad">Load</Button>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                </StackPanel>
                            </GroupBox>

                            <GroupBox x:Name="AnimationData" IsEnabled="True" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label>Animation Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel  Orientation="Horizontal">
                                        <TextBlock Width="105"> Animation Count : </TextBlock>
                                        <TextBlock x:Name="AnimationCount" Width="15" HorizontalAlignment="Center">0</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel  Orientation="Horizontal">
                                        <TextBlock Width="105"> Animation Index :</TextBlock>
                                        <TextBlock x:Name="AnimationIndex" Width="15" HorizontalAlignment="Center">0</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="105">Current Animation</TextBlock>
                                        <ComboBox x:Name="AnimationComboBox" Width="100" VerticalAlignment="Center">
                                            
                                        </ComboBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="CellData" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label>Cell Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal" >
                                        <TextBlock Width="10"></TextBlock>
                                        <TextBlock Foreground="White" Width="70" VerticalAlignment="Center" HorizontalAlignment="Center">Cell Count : </TextBlock>
                                        <TextBlock x:Name="CellCount">0</TextBlock>
                                    </StackPanel>
                                </StackPanel>
                            </GroupBox>
                            <GroupBox x:Name="LightData" Visibility="Collapsed">
                                <GroupBox.Header>
                                    <Label>Light Data</Label>
                                </GroupBox.Header>
                                <StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"></TextBlock>
                                        <TextBlock Foreground="White" Width="70" VerticalAlignment="Center" HorizontalAlignment="Center">Light Type :</TextBlock>
                                        <ComboBox  x:Name="LightType" Width="130" VerticalAlignment="Top" SelectedIndex="0" SelectionChanged="LightType_Selected">
                                            <ComboBoxItem Content="Directional" />
                                            <ComboBoxItem Content="PointLight"/>
                                            <ComboBoxItem Content="SpotLight" />
                                        </ComboBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Direction</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionX_MouseDown">X</TextBlock>
                                        <TextBox x:Name="DirectionX" Width="50" TextChanged="DirectionX_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionY_MouseDown">Y</TextBlock>
                                        <TextBox x:Name="DirectionY" Width="50" TextChanged="DirectionY_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">Z</TextBlock>
                                        <TextBox x:Name="DirectionZ" Width="50" TextChanged="DirectionZ_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Position</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionX_MouseDown">X</TextBlock>
                                        <TextBox x:Name="LightPositionX" Width="50" TextChanged="LightPositionX_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionY_MouseDown">Y</TextBlock>
                                        <TextBox x:Name="LightPositionY" Width="50" TextChanged="LightPositionY_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">Z</TextBlock>
                                        <TextBox x:Name="LightPositionZ" Width="50" TextChanged="LightPositionZ_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Ambient</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionX_MouseDown">R</TextBlock>
                                        <TextBox x:Name="AmbientR" Width="32" TextChanged="AmbientR_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionY_MouseDown">G</TextBlock>
                                        <TextBox x:Name="AmbientG" Width="32" TextChanged="AmbientG_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">B</TextBlock>
                                        <TextBox x:Name="AmbientB" Width="32" TextChanged="AmbientB_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">A</TextBlock>
                                        <TextBox x:Name="AmbientA" Width="32" TextChanged="AmbientA_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Ambient Intensity</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <Slider x:Name="AmbientIntensitySlider" Width="175" Maximum="1" TickPlacement="BottomRight" TickFrequency="0.05" IsSnapToTickEnabled="True"
                                                ValueChanged="AmbientIntensity_ValueChanged"/>
                                        <TextBlock Width="2"/>
                                        <TextBox x:Name="AmbientIntensityTextBox" Width="32" TextAlignment="Center">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Diffuse</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionX_MouseDown">R</TextBlock>
                                        <TextBox x:Name="DiffuseR" Width="32" TextChanged="DiffuseR_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionY_MouseDown">G</TextBlock>
                                        <TextBox x:Name="DiffuseG" Width="32" TextChanged="DiffuseG_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">B</TextBlock>
                                        <TextBox x:Name="DiffuseB" Width="32" TextChanged="DiffuseB_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">A</TextBlock>
                                        <TextBox x:Name="DiffuseA" Width="32" TextChanged="DiffuseA_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Diffuse Intensity</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <Slider x:Name="DiffuseIntensitySlider" Width="175" Maximum="1" TickPlacement="BottomRight" TickFrequency="0.05" IsSnapToTickEnabled="True"
                                                 ValueChanged="DiffuseIntensity_ValueChanged"/>
                                        <TextBlock Width="2"/>
                                        <TextBox  x:Name="DiffuseIntensityTextBox" Width="32" TextAlignment="Center">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Specular</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionX_MouseDown">R</TextBlock>
                                        <TextBox x:Name="SpecularR" Width="32" TextChanged="SpecularR_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionY_MouseDown">G</TextBlock>
                                        <TextBox x:Name="SpecularG" Width="32" TextChanged="SpecularG_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">B</TextBlock>
                                        <TextBox x:Name="SpecularB" Width="32" TextChanged="SpecularB_TextChanged">0</TextBox>
                                        <TextBlock Width="20" HorizontalAlignment="Center" TextAlignment="Center" MouseDown="PositionZ_MouseDown">A</TextBlock>
                                        <TextBox x:Name="SpecularA" Width="32" TextChanged="SpecularA_TextChanged">0</TextBox>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
                                        <TextBlock Width="10"/>
                                        <TextBlock >Specular Intensity</TextBlock>
                                    </StackPanel>
                                    <TextBlock Height="5"/>
                                    <StackPanel Orientation="Horizontal">
