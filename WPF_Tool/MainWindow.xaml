                    <StackPanel Orientation="Horizontal">
                        <TextBlock Width="3"></TextBlock>
                        <Button Width="120" Height="25" HorizontalAlignment="Left">Save</Button>
                        <TextBlock Width="5"></TextBlock>
                        <Button Width="120" Height="25" HorizontalAlignment="Right">Load</Button>
                    </StackPanel>
                </StackPanel>
            </TabItem>
        </TabControl>
        <TabControl Grid.Row="2" Grid.ColumnSpan="2">
            <TabItem Header="Project">
                <StackPanel Orientation="Horizontal">
                    <TreeView Width="150">
                        <TreeViewItem Header="Assets">
                            <TreeViewItem Header="Mesh" MouseDown="Assets_Mesh_MouseDown"/>

                            <TreeViewItem Header="Texture" MouseDown="Assets_Texture_MouseDown"/>
                        </TreeViewItem>
                        <TreeViewItem Header="Datas">
                            <TreeViewItem Header="HierarchyData" MouseDown="Datas_HierarchyData_MouseDown"/>
                            <TreeViewItem Header="GameObjectData"/>
                            <TreeViewItem Header="MeshData" />
                        </TreeViewItem>
                    </TreeView>
                    <ScrollViewer VerticalScrollBarVisibility="Auto"
                                  Width="610" Height="200">
                        <WrapPanel x:Name="ProjectItems">
                        </WrapPanel>
                    </ScrollViewer>
                </StackPanel>
            </TabItem>
            <TabItem Header="Console">
                <StackPanel Orientation="Horizontal">
                    <ScrollViewer x:Name ="ConsoleScrollViewer"  
                                  Width="400" Height="200"
                                  VerticalScrollBarVisibility="Auto" 
                                  Grid.ColumnSpan="2">
                        <WrapPanel  x:Name="ConsoleMessages" Orientation="Vertical">
                        </WrapPanel>
                    </ScrollViewer>
                    <StackPanel Width="360">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Foreground="White" Width="130"> EditorFrame</TextBlock>
                            <TextBlock Foreground="White" x:Name="EditorFrame">0</TextBlock>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Foreground="White" Width="130">DxScreenSize</TextBlock>
                            <TextBlock Foreground="White" Width="20">X :</TextBlock>
                            <TextBlock Foreground="White"  x:Name="DxScreenSizeX" Width="80">0</TextBlock>
                            <TextBlock Foreground="White"  Width="20">Y :</TextBlock>
                            <TextBlock Foreground="White"  x:Name="DxScreenSizeY" Width="80">0</TextBlock>

                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Foreground="White" Width="130">DXMousePosition</TextBlock>
                            <TextBlock Foreground="White"  Width="20">X :</TextBlock>
                            <TextBlock Foreground="White" x:Name="DXMousePosX" Width="80">0</TextBlock>
                            <TextBlock Foreground="White"  Width="20">Y :</TextBlock>
                            <TextBlock Foreground="White" x:Name="DXMousePosY" Width="80">0</TextBlock>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Foreground="White" Width="130">EditorMousePosition</TextBlock>
                            <TextBlock Foreground="White"  Width="20">X :</TextBlock>
                            <TextBlock Foreground="White" x:Name="EditorMousePosX" Width="80">0</TextBlock>
                            <TextBlock Foreground="White"  Width="20">Y :</TextBlock>
                            <TextBlock Foreground="White" x:Name="EditorMousePosY" Width="80">0</TextBlock>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>
            </TabItem>
        </TabControl>
    </Grid>


</Window>
